<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/outgoingdetail"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="20dp">

        <TextView
            android:id="@+id/outgoingdetailorder_no"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="订单号："
            android:textColor="@color/balck"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/outgoingdetailclient_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="订单号："
            android:textColor="@color/balck"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/outgoingdetaillinkman"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="订单号："
            android:textColor="@color/balck"
            android:textSize="18dp" />
        <TextView
            android:id="@+id/outgoingdetailclient_address"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="订单号："
            android:textColor="@color/balck"
            android:textSize="18dp" />
        <TextView
            android:id="@+id/outgoingdetailord_date"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="订单号："
            android:textColor="@color/balck"
            android:textSize="18dp" />
    </LinearLayout>

</LinearLayout>